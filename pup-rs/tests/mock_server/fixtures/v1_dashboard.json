{
  "id": "abc-def-ghi",
  "title": "Production Overview",
  "description": "Main production dashboard with key metrics",
  "url": "/dashboard/abc-def-ghi/production-overview",
  "layout_type": "ordered",
  "author_handle": "test@example.com",
  "created_at": "2024-03-15T08:00:00.000000+00:00",
  "modified_at": "2025-01-20T16:30:00.000000+00:00",
  "is_read_only": false,
  "widgets": [
    {
      "id": 1001,
      "definition": {
        "type": "timeseries",
        "title": "CPU Usage",
        "requests": [
          {
            "q": "avg:system.cpu.user{env:production} by {host}",
            "display_type": "line",
            "style": {
              "palette": "dog_classic"
            }
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 0,
        "width": 6,
        "height": 3
      }
    },
    {
      "id": 1002,
      "definition": {
        "type": "query_value",
        "title": "Request Rate",
        "requests": [
          {
            "q": "sum:trace.http.request.hits{env:production}.as_rate()",
            "aggregator": "avg"
          }
        ],
        "precision": 2
      },
      "layout": {
        "x": 6,
        "y": 0,
        "width": 3,
        "height": 3
      }
    }
  ],
  "template_variables": [
    {
      "name": "env",
      "prefix": "env",
      "default": "production"
    },
    {
      "name": "host",
      "prefix": "host",
      "default": "*"
    }
  ],
  "notify_list": ["test@example.com"],
  "reflow_type": "auto"
}
